{
  "manifest_version": 2,
  "name": "QuickBrowse",
  "description": "Quickly browse to bookmarks (web pages) using keyboard shortcuts",
  "version": "2.1.0",
  "homepage_url": "https://github.com/rob1709/quickBrowse",
  "applications": {
    "gecko": {
      "id": "{a4e116fc-57c7-47ed-81aa-d37c30128dab}"
    }
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "QuickBrowse",
    "default_icon": {
      "48": "logo48.png",
      "96": "logo96.png"
    }
  },
  "icons": {
    "48": "logo48.png",
    "96": "logo96.png"
  },
  "commands": {
  "_execute_browser_action": {
    "suggested_key": {
      "default": "Ctrl+Q",
      "mac": "MacCtrl+Q"
    }
  }
}

}